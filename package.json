{
  "name": "4wfs",
  "version": "1.10.5",
  "engines": {
    "node": "5.7.0"
  },
  "description": "Elite Guard Training - 4WFS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliteguardtraining/4wfs.git"
  },
  "bugs": {
    "url": "https://github.com/eliteguardtraining/4wfs/issues"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "NODE_ENV=production node ./src/server/server.babel.js",
    "debug:prod": "NODE_ENV=production node-debug ./src/server/server.babel.js --verbose",
    "dev": "NODE_ENV=development node ./src/server/server.babel.js",
    "debug:dev": "NODE_ENV=development node-debug ./src/server/server.babel.js --verbose",
    "build": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\"",
    "bs": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\" \"npm run start\"",
    "quickstart": "rimraf build && concurrently \"npm run build:db\" \"npm run build:client\" \"npm run build:server\" \"npm run start\"",
    "build:client": "NODE_ENV=production webpack -p --config ./webpack/prod.babel.js",
    "build:server": "NODE_ENV=production webpack -p --config ./webpack/server.babel.js",
    "build:db": "node ./src/server/database/setupDB.babel.js",
    "update:db": "node ./src/server/database/setupDB.babel.js true",
    "test": "NODE_ENV=testing ava ./src/**/__tests___/**/*.js --verbose",
    "lint": "xo --fix 'src/**/*.js' 'tests/*.js' 'webpack/*.js'",
    "profile": "NODE_ENV=production webpack --config ./webpack/prod.babel.js --profile --json > stats.json"
  },
  "author": "Patrick Scott",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.4.0",
    "ava": "0.13.0",
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-static-fs": "1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
    "concurrently": "2.0.0",
    "css-loader": "0.23.1",
    "eslint": "2.4.0",
    "eslint-config-xo-react": "0.5.0",
    "eslint-plugin-react": "4.2.3",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-style-loader": "1.0.0",
    "file-loader": "0.8.5",
